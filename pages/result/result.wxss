.share-view, .save-view {
  display: inline-block;
  font-size: 22rpx;
  line-height: 34rpx;
  color: #fff;
  text-align: center;
}

.share-view {
  margin-right: 99rpx;
  position: relative;
  display: inline-block;
  margin-left: auto;
  padding-left: 0;
  padding-right: 0;
  box-sizing: border-box;
  font-size: 22rpx;
  text-align: center;
  text-decoration: none;
  line-height: normal;
  border-radius: 0;
  -webkit-tap-highlight-color: transparent;
  overflow: hidden;
  color: #fff;
  background-color: transparent;
  border: none;
}

